Cell[BoxData[{
 RowBox[{
  RowBox[{"roll", "[", 
   RowBox[{"m_", ",", "ind_"}], "]"}], ":=", 
  RowBox[{"Join", "[", 
   RowBox[{
    RowBox[{"m", "\[LeftDoubleBracket]", 
     RowBox[{"ind", ";;"}], "\[RightDoubleBracket]"}], ",", 
    RowBox[{"m", "\[LeftDoubleBracket]", 
     RowBox[{";;", 
      RowBox[{"ind", "-", "1"}]}], "\[RightDoubleBracket]"}]}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"dirs", "=", 
